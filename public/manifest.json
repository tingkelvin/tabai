{
    "name": "Tab",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Tab your life to the future1",
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["vendor.js", "content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_title": "React Extension"
    },
    "background": {
      "service_worker": "background.js"
    },
    "web_accessible_resources": [
      {
        "resources": ["*.js", "*.css"],
        "matches": ["<all_urls>"]
      }
    ],
    "icons": {
      "16": "icons/16x16.png",
      "48": "icons/48x48.png",
      "128": "icons/128x128.png"
    }
  }